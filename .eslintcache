button>\r\n                                <button className=\"btn btn-dark m-2\" type=\"button\" name=\"pantalla\" value=\"Limpiar\" onClick={cambiarPantalla} >Limpiar</button>\r\n                                <button className=\"btn btn-dark m-2\" type=\"submit\" name=\"pantalla\" value=\"Buscar\" onClick={cambiarPantalla} >Buscar</button>\r\n                            </div>\r\n\r\n                            : pantalla === \"Nuevo\" ?\r\n                                <div className=\"col-md-5 m-2\">\r\n                                    <button className=\"btn btn-dark m-2\" type=\"button\" name=\"pantalla\" value=\"Consultar\" onClick={cambiarPantalla} >Consultar</button>\r\n                                    <button className=\"btn btn-dark m-2\" type=\"button\" name=\"pantalla\" value=\"Limpiar\" onClick={cambiarPantalla} >Limpiar</button>\r\n                                    <button className=\"btn btn-dark m-2\" type=\"submit\" name=\"pantalla\" value=\"Guardar\" onClick={cambiarPantalla} >Guardar</button>\r\n                                </div>\r\n\r\n                                :\r\n                                <div className=\"col-md-5 m-2\">\r\n                                    <button className=\"btn btn-dark m-2\" type=\"button\" name=\"pantalla\" value=\"Consultar\" onClick={cambiarPantalla} >Consultar</button>\r\n                                    <button className=\"btn btn-dark m-2\" type=\"button\" name=\"pantalla\" value=\"Limpiar\" onClick={cambiarPantalla} >Limpiar</button>\r\n                                    <button className=\"btn btn-dark m-2\" type=\"submit\" name=\"pantalla\" value=\"Actualizar\" onClick={cambiarPantalla} >Actualizar</button>\r\n                                </div>\r\n                    }\r\n\r\n                </form>\r\n            </div>\r\n            <hr className=\"my-4\" />\r\n            <br />\r\n            {pantalla === \"Buscar\" ? <Detalle respuesta_json={detalle} imprimir={imprimir} pag={paginado} /> : \"\"}\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Inventario;\r\n","C:\\Users\\alejo\\Documents\\cosas alejandro\\cursoReact\\easy\\src\\componentes\\inventario\\Detalle.js",[],"C:\\Users\\alejo\\Documents\\cosas alejandro\\cursoReact\\easy\\src\\componentes\\inventario\\CardProducto.js",[],"C:\\Users\\alejo\\Documents\\cosas alejandro\\cursoReact\\easy\\src\\componentes\\recibocaja\\Recibocaja.js",["243","244","245"],"C:\\Users\\alejo\\Documents\\cosas alejandro\\cursoReact\\easy\\src\\componentes\\recibocaja\\Detalle.js",[],"C:\\Users\\alejo\\Documents\\cosas alejandro\\cursoReact\\easy\\src\\componentes\\popupcliente\\Detalle.js",["246"],"import React, { useState, useEffect } from 'react';\r\n\r\nfunction Detalle(props) {\r\n    const resultado = props.respuesta_json;\r\n    //console.log(resultado.result);\r\n    const [anchoVentana, setAnchoVentana] = useState(window.innerWidth);\r\n\r\n    useEffect(() => {\r\n        const handleResize = () => setAnchoVentana(window.innerWidth)\r\n        window.addEventListener('resize', handleResize);\r\n\r\n        return () => {\r\n            //console.log('return useEffect');\r\n            window.removeEventListener('resize', handleResize);\r\n        }\r\n    }, []);\r\n\r\n    return (\r\n        <div>\r\n            <h1>Resultado</h1>\r\n\r\n            <div className=\"list-group\">\r\n                {!resultado.result ? \"cargando...\" : resultado.result.map((cliente, index) => {\r\n                    return <div key={index} style={{cursor: 'pointer'}} className=\"list-group-item list-group-item-action bg-dark text-white border-white\" onClick={() => props.enviar(cliente.cli_codigo,cliente.cli_nombre1,cliente.cli_apelli1)}>\r\n                        <div className=\"d-flex w-100 justify-content-between\">\r\n                            <h4 className=\"mb-1\">{cliente.cli_nombre1+\" \"+cliente.cli_apelli1}</h4>\r\n                        </div>\r\n                        <p classN